{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ]
  },
  "watch": [
    "scripts/**/*.js",
    "styles/**/*.scss"
  ],
  "publicPath": "/",
  "devUrl": "http://localhost:3000",
  "proxyUrl": "http://localhost:3100",
  "cacheBusting": "[name]_[hash:8]",
  "browsers": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ]
}
